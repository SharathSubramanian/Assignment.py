#File operations

#1)r+ - read and write mode

file=open("file.txt","r+")
file.write("Batch: Feb")
print(file.read())

#2)w+ - write and read mode

file1=open("file1.txt","w+")
file1.write("HELLO")
file1.close()
file1=open("file1.txt","r")
print(file1.read())


#3)a+ - append and read mode

file2=open("file.txt","a+")
file2.write("\n"+"int is basic")
file2.close()
file2=open("file.txt","r")
print(file2.read())
